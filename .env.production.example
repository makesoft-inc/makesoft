# Makesoft.io Production Environment Variables
# Copy this file to .env on production server and fill in actual values

# Domain Configuration
DOMAIN_BASE=makesoft.io

# MongoDB
MONGO_URI=mongodb://mongo:27017/makesoft
MONGO_ROOT_USERNAME=admin
MONGO_ROOT_PASSWORD=__REPLACE_ME__

# JWT Authentication
JWT_SECRET=__REPLACE_ME__
JWT_EXPIRES_IN=7d

# OAuth - Google
GOOGLE_CLIENT_ID=__REPLACE_ME__
GOOGLE_CLIENT_SECRET=__REPLACE_ME__
GOOGLE_REDIRECT_URI=https://auth.makesoft.io/oauth/google/callback

# OAuth - GitHub
GITHUB_CLIENT_ID=__REPLACE_ME__
GITHUB_CLIENT_SECRET=__REPLACE_ME__
GITHUB_REDIRECT_URI=https://auth.makesoft.io/oauth/github/callback

# Cloudflared Tunnel
TUNNEL_ID=ef034f3f-ad26-44ec-bf16-e3131fb09d55

# Node Environment
NODE_ENV=production

